function isIE() {
    var ua = window.navigator.userAgent;

    var msie = ua.indexOf('MSIE ');
    if (msie > 0) {
        // IE 10 or older
        return true;
    }

    var trident = ua.indexOf('Trident/');
    if (trident > 0) {
        // IE 11
        return true;
    }

    // other browser
    return false;
}

if (isIE()) {
    if (window.location.href !== window.location.origin + '/browser-not-supported.html') {
        window.location.href = window.location.origin + '/browser-not-supported.html'
    }
} else {
    if (window.location.href === window.location.origin + '/browser-not-supported.html') {
        window.location.href = window.location.origin;
    }
}